<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/homeFragmentId">

    <fragment
        android:id="@+id/homeFragmentId"
        android:name="com.agaba.intellectsoftnavigation.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/servicesFragmentId"
        android:name="com.agaba.intellectsoftnavigation.ServicesFragment"
        android:label="fragment_services"
        tools:layout="@layout/fragment_services" />
    <fragment
        android:id="@+id/technologiesFragmentId"
        android:name="com.agaba.intellectsoftnavigation.TechnologiesFragment"
        android:label="fragment_technologies"
        tools:layout="@layout/fragment_technologies" />
    <fragment
        android:id="@+id/industriesFragmentId"
        android:name="com.agaba.intellectsoftnavigation.IndustriesFragment"
        android:label="fragment_industries"
        tools:layout="@layout/fragment_industries" />
    <fragment
        android:id="@+id/companyFragmentId"
        android:name="com.agaba.intellectsoftnavigation.CompanyFragment"
        android:label="fragment_company"
        tools:layout="@layout/fragment_company" />
    <fragment
        android:id="@+id/clientsFragmentId"
        android:name="com.agaba.intellectsoftnavigation.ClientsFragment"
        android:label="fragment_clients"
        tools:layout="@layout/fragment_clients" />
</navigation>